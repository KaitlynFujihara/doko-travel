<h2>Account Details</h2>
<div class="length">
  <% @account.preferences.each do |t| %>
    <h5>Trip Duration:</h5>
    <p><%= t.length %> days</p>
    <h5>Interests:</h5>
    <% t.interests.each do |x| %>
    <p><%= x.interest_title %></p>
    <%end%>
    <h5>This is my first time to Japan:</h5>
    <p><%= t.firsttime %></p>
  <% end %>
</div>
